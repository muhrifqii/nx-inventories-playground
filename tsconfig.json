{
  "compilerOptions": {
    "jsx": "react-jsx",
    "allowJs": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "strict": true,
    "types": ["vite/client"],
    "useDefineForClassFields": true
  },
  "extends": "./tsconfig.base.json",
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs"
    }
  }
}
